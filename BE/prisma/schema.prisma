generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id String @id @default(uuid())
  username String @unique @db.VarChar(100)
  password String @unique @db.VarChar(100)
  email  String @db.VarChar(100)
  token String? @db.VarChar(100)
  @@map("users")
}
